{
  "name": "egg-staticlocal",
  "version": "0.8.0",
  "description": "一个egg插件，用于开发阶段帮你起可本地调试的静态服务",
  "eggPlugin": {
    "name": "staticlocal"
  },
  "env": [
    "local"
  ],
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin"
  ],
  "dependencies": {
    "@babel/core": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "autoprefixer": "^7.2.5",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.0-beta.0",
    "commander": "^2.13.0",
    "css-hot-loader": "^1.3.6",
    "css-loader": "^0.28.9",
    "egg-core": "^4.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "globby": "^7.1.1",
    "is-type-of": "^1.2.0",
    "koa-webpack-dev-middleware": "^2.0.2",
    "koa-webpack-hot-middleware": "^1.0.3",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "map-json-webpack-plugin": "^1.2.0",
    "mkdirp": "^0.5.1",
    "postcss-loader": "^2.1.0",
    "rucksack-css": "^1.0.2",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "coffee": "^4.1.0",
    "egg": "^2.2.0",
    "egg-bin": "^4.3.7",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "egg-subapp": "^1.2.0",
    "egg-view-nunjucks": "^2.1.6",
    "eslint": "^4.14.0",
    "eslint-config-egg": "^6.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eventsource": "^1.0.5",
    "rimraf": "^2.6.2",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "bin": {
    "staticlocal-build": "bin/build.js"
  },
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "lint-local": "eslint . --fix",
    "ci": "egg-bin pkgfiles --check && npm run lint && npm run cov",
    "pkgfiles": "egg-bin pkgfiles",
    "autod": "autod"
  },
  "files": [
    "app",
    "config",
    "agent.js",
    "app.js",
    "bin",
    "lib"
  ],
  "ci": {
    "version": "8, 9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yongbo000/egg-staticlocal.git"
  },
  "bugs": {
    "url": "https://github.com/yongbo000/egg/issues"
  },
  "homepage": "https://github.com/yongbo000/egg-staticlocal#readme",
